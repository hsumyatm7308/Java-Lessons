package Collection_Exercise;

public class Book {
		private int ISBN;
		private String title;
		private String author;
		private boolean isIssued;
		
		public Book(){}
		public Book(int ISBN, String title, String author, boolean isIssued) {
			this.ISBN = ISBN;
			this.title = title;
			this.author = author;
			this.isIssued = isIssued;
		}

}
